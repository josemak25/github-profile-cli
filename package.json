{
  "name": "github-profile-cli",
  "version": "1.0.0",
  "description": "A cli tool that gets user github details",
  "main": "build/index",
  "scripts": {
    "postinstall": "tsc",
    "dev": "nodemon",
    "start": "tsc && node build/index",
    "format": "prettier --write \"**/*.{js,ts,tsx,json}\"",
    "test": "jest",
    "test:coverage": "npm test --coverage",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "keywords": [],
  "author": "Amakiri Joseph Lucky",
  "license": "ISC",
  "dependencies": {
    "@octokit/core": "^4.1.0",
    "inquirer": "^8.0.0",
    "postinstall": "^0.7.4",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "babel-jest": "^26.6.3",
    "chalk": "4.1.2",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.0"
  }
}
